var tadclip = function ($obj) {
  console.log($obj);
  $base = $obj.contentWindow.document.querySelector('html');
  $body = $obj.contentDocument.body;
  if ($body.innerHTML.replace(/\s/g, '') === '') {
    $obj.parentNode.parentNode.parentNode.removeChild($obj.parentNode.parentNode);
    /*$obj.style.display = 'block';
    $obj.setAttribute('height', 0);*/
  } else if ($body) {
    html = $body.innerHTML.replace(/^([\s\S]*?)(<br>\s*)?$/, "$1");
    html = '<style>body{font-family:Arial,sans-serif;font-size:14px;line-height:18px;padding-top:10px}span{display:block;border-bottom:1px solid #E5E5E5;padding-bottom:10px}a{color:#05C;text-decoration:none;padding:4px 0}a span{border-bottom:none;padding-bottom:2px}b{padding:0 0 0 18px;background:url(/assets/templates/gihyojp2007/image/pr13x13.gif) 0px 0 no-repeat}a:hover{color:#38C;text-decoration:underline}body>br{display:none}</style>' + html;
    $body.innerHTML = html;
    h = $base.offsetHeight;
    $obj.setAttribute('height', h);
  }
}
var $ptt = document.getElementById('primaryTopTextPr');
if ($ptt) {
  $ptt.style.height = 'auto';
}
setTimeout(function(){
  var $frm = document.querySelectorAll('#primaryTopTextPr iframe');
  for (idx in $frm) {
    if (isNaN(idx)) {
      continue;
    }
    if ($frm[idx] === null) {
      continue;
    } else if ($frm[idx].contentWindow.document.readyState == 'complete') {
      tadclip($frm[idx]);
    } else {
      $frm[idx].addEventListener('load', function(e) {
        tadclip(e.target);
      });
    }
  }
  setTimeout(function(){
    $frm = document.querySelectorAll('#primaryTopTextPr iframe');
